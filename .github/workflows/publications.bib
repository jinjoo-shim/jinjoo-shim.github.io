}

@article{friedenreich_physical_2016,
	title = {Physical {Activity} and {Cancer} {Outcomes}: {A} {Precision} {Medicine} {Approach}},
	volume = {22},
	issn = {1078-0432, 1557-3265},
	shorttitle = {Physical {Activity} and {Cancer} {Outcomes}},
	url = {https://aacrjournals.org/clincancerres/article/22/19/4766/249217/Physical-Activity-and-Cancer-Outcomes-A-Precision},
	doi = {10.1158/1078-0432.CCR-16-0067},
	abstract = {Abstract
            There is increasing interest in applying a precision medicine approach to understanding exercise as a potential treatment for cancer. We aimed to inform this new approach by appraising epidemiologic literature relating postdiagnosis physical activity to cancer outcomes overall and by molecular/genetic subgroups. Across 26 studies of breast, colorectal, and prostate cancer patients, a 37\% reduction was seen in risk of cancer-specific mortality, comparing the most versus the least active patients (pooled relative risk = 0.63; 95\% confidence interval: 0.54‚Äì0.73). Risks of recurrence or recurrence/cancer-specific death (combined outcome) were also reduced based on fewer studies. We identified ten studies of associations between physical activity and cancer outcomes by molecular or genetic markers. Two studies showed statistically significant risk reductions in breast cancer mortality/recurrence for the most (versus least) physically active estrogen receptor‚Äìpositive/progesterone receptor‚Äìpositive (ER+/PR+) patients, while others showed risk reductions among ER‚àíPR‚àí and triple-negative patients. In colorectal cancer, four studies showed statistically significant risk reductions in cancer-specific mortality for patients with high (versus low) physical activity and P21 expression, P27 expression, nuclear CTNNB1‚àí, PTGS2 (COX-2)+, or IRS1 low/negative status. One prostate cancer study showed effect modification by Gleason score. As a means to enhance this evidence, future observational studies are needed that will measure physical activity objectively before and after diagnosis, use standardized definitions for outcomes, control for competing risks, assess nonlinear dose‚Äìresponse relations, and consider reverse causality. Ultimately, randomized controlled trials with clinical cancer outcomes and a correlative component will provide the best evidence of causality, relating exercise to cancer outcomes, overall and for molecular and genetic subgroups. Clin Cancer Res; 22(19); 4766‚Äì75. ¬©2016 AACR.},
	language = {en},
	number = {19},
	urldate = {2023-01-23},
	journal = {Clinical Cancer Research},
	author = {Friedenreich, Christine M. and Neilson, Heather K. and Farris, Megan S. and Courneya, Kerry S.},
	month = oct,
	year = {2016},
	pages = {4766--4775},
	file = {Friedenreich et al. - 2016 - Physical Activity and Cancer Outcomes A Precision.pdf:/Users/jishim/Zotero/storage/2H9MA7G5/Friedenreich et al. - 2016 - Physical Activity and Cancer Outcomes A Precision.pdf:application/pdf},
}

