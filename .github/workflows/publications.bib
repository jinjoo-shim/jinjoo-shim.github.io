@article{shim_wearable-based_2023-3,
	title = {Wearable-based accelerometer activity profile as digital biomarker of inflammation, biological age, and mortality using hierarchical clustering analysis in {NHANES} 2011‚Äì2014},
	volume = {13},
	copyright = {2023 The Author(s)},
	issn = {2045-2322},
	url = {https://www.nature.com/articles/s41598-023-36062-y},
	doi = {10.1038/s41598-023-36062-y},
	abstract = {Repeated disruptions in circadian rhythms are associated with implications for health outcomes and longevity. The utilization of wearable devices in quantifying circadian rhythm to elucidate its connection to longevity, through continuously collected data remains largely unstudied. In this work, we investigate a data-driven segmentation of the 24-h accelerometer activity profiles from wearables as a novel digital biomarker for longevity in 7,297 U.S. adults from the 2011‚Äì2014 National Health and Nutrition Examination Survey. Using hierarchical clustering, we identified five clusters and described them as follows: ‚ÄúHigh activity‚Äù, ‚ÄúLow activity‚Äù, ‚ÄúMild circadian rhythm (CR) disruption‚Äù, ‚ÄúSevere CR disruption‚Äù, and ‚ÄúVery low activity‚Äù. Young adults with extreme CR disturbance are seemingly healthy with few comorbid conditions, but in fact associated with higher white blood cell, neutrophils, and lymphocyte counts (0.05‚Äì0.07 log-unit, all p‚Äâ{\textless}‚Äâ0.05) and accelerated biological aging (1.42¬†years, p‚Äâ{\textless}‚Äâ0.001). Older adults with CR disruption are significantly associated with increased systemic inflammation indexes (0.09‚Äì0.12 log-unit, all p‚Äâ{\textless}‚Äâ0.05), biological aging advance (1.28¬†years, p‚Äâ=‚Äâ0.021), and all-cause mortality risk (HR‚Äâ=‚Äâ1.58, p‚Äâ=‚Äâ0.042). Our findings highlight the importance of circadian alignment on longevity across all ages and suggest that data from wearable accelerometers can help in identifying at-risk populations and personalize treatments for healthier aging.},
	language = {en},
	number = {1},
	urldate = {2023-12-31},
	journal = {Scientific Reports},
	author = {Shim, Jinjoo and Fleisch, Elgar and Barata, Filipe},
	month = jun,
	year = {2023},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Biomarkers, Machine learning, Risk factors, Disease prevention},
	pages = {9326},
	file = {Full Text PDF:/Users/jishim/Zotero/storage/SGQLBNAM/Shim et al. - 2023 - Wearable-based accelerometer activity profile as d.pdf:application/pdf},
}

@article{polito_real-world_2023-1,
	title = {Real-{World} {First}-{Line} {Use} of {Pertuzumab} {With} {Different} {Taxanes} for {Human} {Epidermal} {Growth} {Factor} {Receptor} 2-{Positive} {Metastatic} {Breast} {Cancer}: {A} {Comparative} {Effectiveness} {Study} {Using} {US} {Electronic} {Health} {Records}},
	volume = {19},
	issn = {2688-1535},
	shorttitle = {Real-{World} {First}-{Line} {Use} of {Pertuzumab} {With} {Different} {Taxanes} for {Human} {Epidermal} {Growth} {Factor} {Receptor} 2-{Positive} {Metastatic} {Breast} {Cancer}},
	doi = {10.1200/OP.22.00565},
	abstract = {PURPOSE: On the basis of the results from CLEOPATRA, pertuzumab plus trastuzumab and chemotherapy is the first-line standard of care for human epidermal growth factor receptor 2 (HER2)-positive metastatic breast cancer (MBC). However, discrepancies have been reported between clinical trial and real-world outcomes. We report real-world outcomes for patients with HER2-positive MBC treated with first-line pertuzumab plus trastuzumab and a taxane in routine clinical practice in the United States.
METHODS: A retrospective analysis was conducted using electronic health record-derived deidentified data from the Flatiron Health database. Patients were grouped according to the first taxane received (paclitaxel/nab-paclitaxel or docetaxel). Median real-world progression-free survival (rwPFS) and overall survival (rwOS) was estimated using Kaplan-Meier methodology. Subgroup analyses were conducted in patients treated with docetaxel who met CLEOPATRA's key eligibility criteria.
RESULTS: We included 1,065 patients; 313 patients received paclitaxel/nab-paclitaxel and 752 received docetaxel. Patients who received paclitaxel/nab-paclitaxel were older, had a worse Eastern Cooperative Oncology Group Performance Status, and had more recurrent metastatic disease compared with the docetaxel group. After adjustment for potential confounders, similar median rwPFS (inverse probability of treatment weighted average treatment effect for the treated [IPTW-ATT] hazard ratio [HR], 1.09; 95\% CI, 0.9 to 1.3; P = .365) and rwOS (IPTW-ATT HR, 1.23; 95\% CI, 0.96 to 1.58; P = .101) was observed between treatment groups. In the subgroup of CLEOPATRA-eligible patients, median rwPFS and rwOS were 16.9 months and 57.8 months, respectively.
CONCLUSION: There was no statistically significant difference in real-world outcomes between patients treated with paclitaxel/nab-paclitaxel and those treated with docetaxel. Selecting patients using key CLEOPATRA eligibility criteria resulted in rwPFS and rwOS similar to those observed in CLEOPATRA, highlighting the importance of ensuring similar patient populations when comparing clinical trial and real-world data.},
	language = {eng},
	number = {7},
	journal = {JCO oncology practice},
	author = {Polito, Letizia and Shim, Jinjoo and Hurvitz, Sara A. and Dang, Chau T. and Knott, Adam and Du Toit, Yolande and Restuccia, Eleonora and Sanglier, Thibaut and Swain, Sandra M.},
	month = jul,
	year = {2023},
	pmid = {37167571},
	pmcid = {PMC10337715},
	keywords = {Female, Humans, Retrospective Studies, Breast Neoplasms, Electronic Health Records, Trastuzumab, Docetaxel, Paclitaxel, Taxoids},
	pages = {435--445},
	file = {Full Text:/Users/jishim/Zotero/storage/TIU23759/Polito et al. - 2023 - Real-World First-Line Use of Pertuzumab With Diffe.pdf:application/pdf},
}

