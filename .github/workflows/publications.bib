	journal = {RMD Open},
	author = {Fenton, Sally A M and Ntoumanis, Nikos and Duda, Joan L and Metsios, George S and Rouse, Peter C and Yu, Chen-an and Kitas, George D and Veldhuijzen van Zanten, Jet J C S},
	month = jul,
