	pages = {1179--1188},
	file = {PubMed Central Full Text PDF:/Users/jishim/Zotero/storage/N84H46T5/Hastings et al. - 2021 - Conceptual and Analytical Overlap Between Allostat.pdf:application/pdf},
}

