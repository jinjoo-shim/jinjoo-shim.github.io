	pages = {e0268141},
	file = {Full Text:/Users/jishim/Zotero/storage/WBXA8F8Z/Peconi et al. - 2022 - Sunproofed study protocol A mixed-methods scoping.pdf:application/pdf},
}

@article{ashinyo_infection_2021,
	title = {Infection prevention and control compliance among exposed healthcare workers in {COVID}-19 treatment centers in {Ghana}: {A} descriptive cross-sectional study},
