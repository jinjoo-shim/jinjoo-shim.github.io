	keywords = {Adult, Aged, Humans, Middle Aged, Young Adult, Adolescent, Reproducibility of Results, physical activity, Wrist, Accelerometry, Exercise, Sedentary Behavior, activity count, activity monitor, MVPA, sedentary behaviour, Hip, Fitness Trackers, Data Analysis},
	pages = {2569--2578},
