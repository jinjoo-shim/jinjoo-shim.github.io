	keywords = {Female, Humans, Retrospective Studies, Breast Neoplasms, Real-world, Ado-Trastuzumab Emtansine, Antineoplastic Combined Chemotherapy Protocols, Brain metastases, Brain Neoplasms, Capecitabine, HER2-Positive metastatic breast cancer, Lapatinib, Lapatinib plus capecitabine, Maytansine, Quinazolines, Receptor, ErbB-2, Trastuzumab, Trastuzumab emtansine},
	pages = {441--450},
	file = {Full Text:/Users/jishim/Zotero/storage/2NU4BDWQ/Sanglier et al. - 2023 - Trastuzumab emtansine vs lapatinib and capecitabin.pdf:application/pdf},
