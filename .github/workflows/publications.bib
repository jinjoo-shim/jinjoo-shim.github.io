	urldate = {2024-02-01},
	journal = {European Journal of Preventive Cardiology},
	author = {Ekblom-Bak, Elin and Halldin, Mats and Vikstr√∂m, Max and Stenling, Andreas and Gigante, Bruna and de Faire, Ulf and Leander, Karin and Hell√©nius, Mai-Lis},
	month = nov,
	year = {2021},
