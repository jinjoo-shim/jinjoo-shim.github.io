	journal = {International Journal of Cancer},
	author = {Huisman, Sander A. and Oklejewicz, Malgorzata and Ahmadi, Ali R. and Tamanini, Filippo and Ijzermans, Jan N.M. and van der Horst, Gijsbertus T.J. and de Bruin, Ron W.F.},
	year = {2015},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ijc.29089},
	keywords = {circadian rhythm, cancer, phase shift},
	pages = {1024--1032},
	file = {Full Text PDF:/Users/jishim/Zotero/storage/7KWKWDK5/Huisman et al. - 2015 - Colorectal liver metastases with a disrupted circa.pdf:application/pdf;Snapshot:/Users/jishim/Zotero/storage/VGSDVLTM/ijc.html:text/html},
