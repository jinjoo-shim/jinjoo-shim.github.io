	volume = {14},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	issn = {2072-6643},
	shorttitle = {Short {Sleep} {Duration} {Was} {Associated} with {Increased} {Regional} {Body} {Fat} in {US} {Adults}},
	url = {https://www.mdpi.com/2072-6643/14/14/2840},
	doi = {10.3390/nu14142840},
	abstract = {Background: The relationship between sleep duration and different regional fat is unclear. We aimed to investigate the association between sleep duration and different regional fat mass (FM) among a population of US adults. Methods: 9413 participants were included from the National Health and Nutrition Examination Survey (NHANES), from 2011 to 2018. The sleep duration was divided into short sleep ({\textless}7 h/day), normal sleep (7‚Äì9 h/day) and long sleep ({\textgreater}9 h/day). Different regional FM was measured by dual-energy X-ray absorptiometry, including trunk FM, arms FM and legs FM. Fat mass index (FMI) was obtained by dividing FM (kg) by the square of body height (m2). Multiple linear regression was used to evaluate the relationship between sleep duration and regional FMI. Results: The mean sleep duration was 7.1 ¬± 1.5 h/day. After adjusting for socio-demographic, lifestyle information, comorbid diseases and medications, short sleepers had higher trunk FMI (Œ≤ = 0.134, 95\% confidence interval (CI): 0.051‚Äì0.216, p = 0.001), arms FMI (Œ≤ = 0.038, 95\% CI: 0.016‚Äì0.06, p {\textless} 0.001) and legs FMI (Œ≤ = 0.101, 95\% CI: 0.044‚Äì0.158, p {\textless} 0.001) compared to normal sleepers, whereas no significant difference was found in long sleepers. The similar results were also observed in men, while short sleepers only had higher arms FM in women (all p {\textless} 0.01). In addition, compared to normal sleepers, short sleepers had higher arms FMI and legs FMI in the obese group (all p {\textless} 0.05), while no relationship was observed in non-obese group. Conclusions: Short sleep duration, but not long sleep duration, was independently related to the increased different regional body fat in US adults, especially in men and those with obesity.},
	language = {en},
	number = {14},
	urldate = {2023-03-28},
	journal = {Nutrients},
	author = {Xu, Chong and Zhao, Song and Yu, Shikai and Tang, Jiamin and Zhang, Han and Xu, Bei and Xu, Yawei and Zhang, Yi},
	month = jan,
	year = {2022},
	note = {Number: 14
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {NHANES, body fat, sleep duration},
	pages = {2840},
	file = {Full Text PDF:/Users/jishim/Zotero/storage/C8VN2CB4/Xu et al. - 2022 - Short Sleep Duration Was Associated with Increased.pdf:application/pdf},
}

