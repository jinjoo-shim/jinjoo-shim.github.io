	pmcid = {PMC4155060},
	pages = {1729--1751},
	file = {PubMed Central Full Text PDF:/Users/jishim/Zotero/storage/L498IBZI/Brenner et al. - 2014 - A review of the application of inflammatory biomar.pdf:application/pdf},
