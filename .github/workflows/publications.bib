	journal = {Medicine and Science in Sports and Exercise},
	author = {Fishman, Ezra I. and Steeves, Jeremy A. and Zipunnikov, Vadim and Koster, Annemarie and Berrigan, David and Harris, Tamara A. and Murphy, Rachel},
