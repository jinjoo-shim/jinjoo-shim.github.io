	keywords = {phenotyping, 24-hour diet recall questionnaires, empirical bayes, heritability, longitudinal data, nutrigenomics, nutritional epidemiology, relative validity},
	pages = {1070511},
	file = {Full Text:/Users/jishim/Zotero/storage/DF7ESKB2/Cole et al. - 2022 - Genetic heritability as a tool to evaluate the pre.pdf:application/pdf},
}

