	pmcid = {PMC8710750},
	pages = {771118},
	file = {PubMed Central Full Text PDF:/Users/jishim/Zotero/storage/JJMB9BLF/Ashkenazi-Hoffnung et al. - 2021 - Differential Serum and Urine CRP, IP-10, and TRAIL.pdf:application/pdf},
