	month = may,
	year = {2023},
	keywords = {recovery, biological age, aging, dynamics, epigenetic aging clocks, stress},
	pages = {807--820.e5},
	file = {ScienceDirect Full Text PDF:/Users/jishim/Zotero/storage/F8UTZU3Q/Poganik et al. - 2023 - Biological age is increased by stress and restored.pdf:application/pdf},
