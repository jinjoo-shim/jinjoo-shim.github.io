	journal = {The Journals of Gerontology: Series A},
	author = {Schrack, Jennifer A and Leroux, Andrew and Fleg, Jerome L and Zipunnikov, Vadim and Simonsick, Eleanor M and Studenski, Stephanie A and Crainiceanu, Ciprian and Ferrucci, Luigi},
