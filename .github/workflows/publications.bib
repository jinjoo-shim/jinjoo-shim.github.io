	journal = {Frontiers in Public Health},
	author = {Liu, Zhizhen and Zhang, Lin and Wu, Jingsong and Zheng, Zhicheng and Gao, Jiahui and Lin, Yongsheng and Liu, Yinghua and Xu, Haihua and Zhou, Yongjin},
	month = oct,
