	author = {Caristia, Silvia and Filigheddu, Nicoletta and Barone-Adesi, Francesco and Sarro, Andrea and Testa, Tommaso and Magnani, Corrado and Aimaretti, Gianluca and Faggiano, Fabrizio and Marzullo, Paolo},
