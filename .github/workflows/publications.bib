	keywords = {Circadian rhythms, Chronobiology, Chronotherapeutics, Drug disposition, Molecular clock, Pharmacodynamics, Systems pharmacology},
	pages = {321--338},
	file = {Full Text PDF:/Users/jishim/Zotero/storage/FDHAG8NT/Ayyar and Sukumaran - 2021 - Circadian rhythms influence on physiology, pharma.pdf:application/pdf},
}

