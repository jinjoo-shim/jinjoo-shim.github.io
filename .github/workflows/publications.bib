@article{pontzer_daily_2021,
	title = {Daily energy expenditure through the human life course},
	volume = {373},
	url = {https://www.science.org/doi/full/10.1126/science.abe5017},
	doi = {10.1126/science.abe5017},
	abstract = {Total daily energy expenditure (‚Äútotal expenditure‚Äù) reflects daily energy needs and is a critical variable in human health and physiology, but its trajectory over the life course is poorly studied. We analyzed a large, diverse database of total expenditure measured by the doubly labeled water method for males and females aged 8 days to 95 years. Total expenditure increased with fat-free mass in a power-law manner, with four distinct life stages. Fat-free mass‚Äìadjusted expenditure accelerates rapidly in neonates to {\textasciitilde}50\% above adult values at {\textasciitilde}1 year; declines slowly to adult levels by {\textasciitilde}20 years; remains stable in adulthood (20 to 60 years), even during pregnancy; then declines in older adults. These changes shed light on human development and aging and should help shape nutrition and health strategies across the life span.},
