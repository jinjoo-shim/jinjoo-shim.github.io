@article{xiao_nonparametric_2022,
	title = {Nonparametric {Parameters} of 24-{Hour} {Rest}‚Äì{Activity} {Rhythms} and {Long}-{Term} {Cognitive} {Decline} and {Incident} {Cognitive} {Impairment} in {Older} {Men}},
