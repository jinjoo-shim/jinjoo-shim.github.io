	urldate = {2023-08-15},
	journal = {International Journal of Epidemiology},
	author = {Hajna, Samantha and White, Tom and Panter, Jenna and Brage, S√∏ren and Wijndaele, Katrien and Woodcock, James and Ogilvie, David and Imamura, Fumiaki and Griffin, Simon J},
