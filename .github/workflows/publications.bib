	journal = {Frontiers in Endocrinology},
	author = {Li, Jingen and Vungarala, Soumya and Somers, Virend K. and Di, Junrui and Lopez-Jimenez, Francisco and Covassin, Naima},
	month = jun,
	year = {2022},
	pages = {907360},
	file = {Full Text:/Users/jishim/Zotero/storage/J9AUSPJL/Li et al. - 2022 - Rest-Activity Rhythm Is Associated With Obesity Ph.pdf:application/pdf},
}

