	month = mar,
	year = {2017},
	keywords = {biochemical indicator, biological specimen, cutoff, fat-soluble vitamin, iron-status indicator, national nutrition survey, water-soluble vitamin},
	pages = {290--307},
	file = {ScienceDirect Full Text PDF:/Users/jishim/Zotero/storage/EEVDZU34/Pfeiffer et al. - 2017 - Challenges and Lessons Learned in Generating and I.pdf:application/pdf;ScienceDirect Snapshot:/Users/jishim/Zotero/storage/5ZHBB64J/S2161831322007189.html:text/html},
}

