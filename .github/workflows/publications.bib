	keywords = {circadian rhythm, carbohydrate oxidation, circadian phase, lipid oxidation, metabolic rate, respiratory quotient, resting energy expenditure},
	pages = {3685--3690.e3},
	file = {ScienceDirect Full Text PDF:/Users/jishim/Zotero/storage/N85593LV/Zitting et al. - 2018 - Human Resting Energy Expenditure Varies with Circa.pdf:application/pdf;ScienceDirect Snapshot:/Users/jishim/Zotero/storage/UMHPYUQT/S0960982218313344.html:text/html},
