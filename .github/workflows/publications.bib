	keywords = {accelerometer, cosinor model, data-driven chronotypes, diurnal registration, functional principal components analysis},
	pages = {1299--1319},
	file = {Accepted Version:/Users/jishim/Zotero/storage/D4R5RR6A/McDonnell et al. - 2022 - Registration of 24-hour accelerometric rest-activi.pdf:application/pdf},
}

