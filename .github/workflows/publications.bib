	pmcid = {PMC6907085},
	pages = {28--46},
	file = {PubMed Central Full Text PDF:/Users/jishim/Zotero/storage/CKXV8HJA/Witt et al. - 2019 - Windows Into Human Health Through Wearables Data A.pdf:application/pdf},
}

