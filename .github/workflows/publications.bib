@article{migueles_calibration_2021,
	title = {Calibration and {Cross}-{Validation} of {Accelerometer} {Cut}-{Points} to {Classify} {Sedentary} {Time} and {Physical} {Activity} from {Hip} and {Non}-{Dominant} and {Dominant} {Wrists} in {Older} {Adults}},
