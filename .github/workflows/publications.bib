Publisher: Nature Publishing Group},
	keywords = {Epidemiology, Risk factors},
	pages = {1385--1391},
	file = {Full Text PDF:/Users/jishim/Zotero/storage/HV3QU7K8/Strain et al. - 2020 - Wearable-device-measured physical activity and fut.pdf:application/pdf},
