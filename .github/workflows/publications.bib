	keywords = {Adult, Aged, Female, Humans, Male, Aged, 80 and over, Sex Factors, Time Factors, Aging, Circadian Rhythm, Body Temperature, NASA Discipline Regulatory Physiology, Non-NASA Center, Sleep Deprivation},
	pages = {933--936},
