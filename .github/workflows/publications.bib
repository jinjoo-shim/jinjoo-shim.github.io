@article{ussery_trends_2021,
	title = {Trends in {Self}-{Reported} {Sitting} {Time} by {Physical} {Activity} {Levels} {Among} {US} {Adults}, {NHANES} 2007/2008‚Äì2017/2018},
	volume = {18},
	issn = {1543-5474, 1543-3080},
	url = {https://journals.humankinetics.com/view/journals/jpah/18/S1/article-pS74.xml},
	doi = {10.1123/jpah.2021-0221},
	abstract = {Background: High levels of sedentary behavior and physical inactivity increase the risk of premature mortality and several chronic diseases. Monitoring national trends and correlates of sedentary behavior and physical inactivity can help identify patterns of risk in the population over time. Methods: The authors used self-reported data from the National Health and Nutrition Examination Surveys (2007/2008‚Äì2017/2018) to estimate trends in US adults‚Äô mean daily sitting time, overall, and stratified by levels of leisure-time and multidomain physical activity, and in the joint prevalence of high sitting time ({\textgreater}8 h/d) and physical inactivity. Trends were tested using orthogonal polynomial contrasts. Results: Overall, mean daily sitting time increased by 19 minutes from 2007/2008 (332 min/d) to 2017/2018 (351 min/d) (Plinear‚Äâ{\textless}‚Äâ.05; Pquadratic‚Äâ{\textless}‚Äâ.05). The highest point estimate occurred in 2013/2014 (426 min/d), with a decreasing trend observed after this point (Plinear‚Äâ{\textless}‚Äâ.05). Similar trends were observed across physical activity levels and domains, with one exception: an overall linear increase was not observed among sufficiently active adults. The mean daily sitting time was lowest among highly active adults compared with less active adults when using the multidomain physical activity measure. Conclusions: Sitting time among adults increased over the study period but decreased in recent years.},
	language = {en},
	number = {S1},
	urldate = {2023-03-21},
	journal = {Journal of Physical Activity and Health},
	author = {Ussery, Emily N. and Whitfield, Geoffrey P. and Fulton, Janet E. and Galuska, Deborah A. and Matthews, Charles E. and Katzmarzyk, Peter T. and Carlson, Susan A.},
	month = aug,
