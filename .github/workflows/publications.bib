	number = {18},
	journal = {The New England Journal of Medicine},
	author = {Lockley, Steven W. and Cronin, John W. and Evans, Erin E. and Cade, Brian E. and Lee, Clark J. and Landrigan, Christopher P. and Rothschild, Jeffrey M. and Katz, Joel T. and Lilly, Craig M. and Stone, Peter H. and Aeschbach, Daniel and Czeisler, Charles A. and {Harvard Work Hours, Health and Safety Group}},
