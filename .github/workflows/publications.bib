	keywords = {Inflammation, Adult, Aged, Disease Progression, Female, Humans, Male, Middle Aged, Prognosis, Aged, 80 and over, Follow-Up Studies, Biomarkers, Predictive Value of Tests, Time Factors, United States, Cause of Death, Chronic Disease, Immunity, Leukocyte Count, Lymphocytes, Neutrophils},
	pages = {464},
