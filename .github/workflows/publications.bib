	urldate = {2022-07-15},
	journal = {Journal of the American Heart Association},
	author = {Dempsey, Paddy C. and Strain, Tessa and Winkler, Elisabeth A. H. and Westgate, Kate and Rennie, Kirsten L. and Wareham, Nicholas J. and Brage, Soren and Wijndaele, Katrien},
