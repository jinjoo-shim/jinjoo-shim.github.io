@article{keadle_impact_2014,
	title = {Impact of accelerometer data processing decisions on the sample size, wear time and physical activity level of a large cohort study},
	volume = {14},
	issn = {1471-2458},
	doi = {10.1186/1471-2458-14-1210},
	abstract = {BACKGROUND: Accelerometers objectively assess physical activity (PA) and are currently used in several large-scale epidemiological studies, but there is no consensus for processing the data. This study compared the impact of wear-time assessment methods and using either vertical (V)-axis or vector magnitude (VM) cut-points on accelerometer output.
METHODS: Participants (7,650 women, mean age 71.4 y) were mailed an accelerometer (ActiGraph GT3X+), instructed to wear it for 7 days, record dates and times the monitor was worn on a log, and return the monitor and log via mail. Data were processed using three wear-time methods (logs, Troiano or Choi algorithms) and V-axis or VM cut-points.
RESULTS: Using algorithms alone resulted in "mail-days" incorrectly identified as "wear-days" (27-79\% of subjects had {\textgreater}7-days of valid data). Using only dates from the log and the Choi algorithm yielded: 1) larger samples with valid data than using log dates and times, 2) similar wear-times as using log dates and times, 3) more wear-time (V, 48.1 min more; VM, 29.5 min more) than only log dates and Troiano algorithm. Wear-time algorithm impacted sedentary time ({\textasciitilde}30-60 min lower for Troiano vs. Choi) but not moderate-to-vigorous (MV) PA time. Using V-axis cut-points yielded {\textasciitilde}60 min more sedentary time and {\textasciitilde}10 min less MVPA time than using VM cut-points.
CONCLUSIONS: Combining log-dates and the Choi algorithm was optimal, minimizing missing data and researcher burden. Estimates of time in physical activity and sedentary behavior are not directly comparable between V-axis and VM cut-points. These findings will inform consensus development for accelerometer data processing in ongoing epidemiologic studies.},
	language = {eng},
	journal = {BMC public health},
	author = {Keadle, Sarah Kozey and Shiroma, Eric J. and Freedson, Patty S. and Lee, I.-Min},
	month = nov,
	year = {2014},
	pmid = {25421941},
	pmcid = {PMC4247661},
	keywords = {Adult, Aged, Female, Humans, Middle Aged, Body Mass Index, Cohort Studies, Time Factors, Algorithms, Accelerometry, Monitoring, Ambulatory, Exercise, Electronic Data Processing, Epidemiologic Measurements, Physical Examination, Sample Size},
	pages = {1210},
	file = {Full Text:/Users/jishim/Zotero/storage/5S3V698D/Keadle et al. - 2014 - Impact of accelerometer data processing decisions .pdf:application/pdf},
}

