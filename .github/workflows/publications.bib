	number = {12},
	urldate = {2022-10-31},
	journal = {PLOS ONE},
	author = {Tleyjeh, Imad M. and Saddik, Basema and AlSwaidan, Nourah and AlAnazi, Ahmed and Ramakrishnan, Rakhee K. and Alhazmi, Deema and Aloufi, Ahmad and AlSumait, Fahad and Berbari, Elie and Halwani, Rabih},
