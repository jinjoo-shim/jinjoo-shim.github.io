	title = {Isotemporal {Substitution} {Analysis} of {Accelerometer}-{Derived} {Sedentary} {Behavior} and {Physical} {Activity} on {Cardiometabolic} {Health} in {Korean} {Adults}: {A} {Population}-{Based} {Cross}-{Sectional} {Study}},
