	keywords = {autonomic nervous system, complexity, distribution entropy, mortality, resting ECG},
	pages = {e018483},
	file = {Full Text PDF:/Users/jishim/Zotero/storage/MAR942GB/Gao et al. - 2021 - Resting Heartbeat Complexity Predicts All‚ÄêCause an.pdf:application/pdf},
