	urldate = {2023-05-03},
	journal = {Nature Medicine},
	author = {Strain, Tessa and Wijndaele, Katrien and Dempsey, Paddy C. and Sharp, Stephen J. and Pearce, Matthew and Jeon, Justin and Lindsay, Tim and Wareham, Nick and Brage, S√∏ren},
	month = sep,
