	month = may,
	year = {2021},
	pages = {e29562},
	file = {Full Text:/Users/jishim/Zotero/storage/494MPZ7W/Clingan et al. - 2021 - Monitoring Health Care Workers at Risk for COVID-1.pdf:application/pdf},
