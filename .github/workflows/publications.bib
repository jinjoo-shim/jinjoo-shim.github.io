Publisher: Nature Publishing Group},
	keywords = {Epidemiology, Risk factors, Cardiovascular diseases},
	pages = {2521--2529},
	file = {Full Text PDF:/Users/jishim/Zotero/storage/9C72DA42/Stamatakis et al. - 2022 - Association of wearable device-measured vigorous i.pdf:application/pdf},
