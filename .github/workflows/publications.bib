	journal = {Progress in Cardiovascular Diseases},
	author = {Sagner, Michael and McNeil, Amy and Puska, Pekka and Auffray, Charles and Price, Nathan D. and Hood, Leroy and Lavie, Carl J. and Han, Ze-Guang and Chen, Zhu and Brahmachari, Samir Kumar and McEwen, Bruce S. and Soares, Marcelo B. and Balling, Rudi and Epel, Elissa and Arena, Ross},
	month = mar,
	year = {2017},
	keywords = {P4 medicine, Systems biology, Systems medicine, Exercise, Allostasis, Allostatic load, Healthspan, Nutrition, Wellness},
	pages = {506--521},
	file = {ScienceDirect Full Text PDF:/Users/jishim/Zotero/storage/PL8ZSIUN/Sagner et al. - 2017 - The P4 Health Spectrum ‚Äì A Predictive, Preventive,.pdf:application/pdf;ScienceDirect Snapshot:/Users/jishim/Zotero/storage/P4BTK7AA/S0033062016300780.html:text/html},
