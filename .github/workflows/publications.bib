	file = {PubMed Central Full Text PDF:/Users/jishim/Zotero/storage/E3PD9IFP/Li et al. - 2021 - Demographic characteristics associated with circad.pdf:application/pdf},
}

@article{fossion_multiscale_2017,
	title = {Multiscale adaptive analysis of circadian rhythms and intradaily variability: {Application} to actigraphy time series in acute insomnia subjects},
