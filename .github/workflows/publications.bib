	urldate = {2023-07-27},
	journal = {Journal of Affective Disorders},
	author = {Pye, Jonathon and Phillips, Andrew JK and Cain, Sean W and Montazerolghaem, Maryam and Mowszowski, Loren and Duffy, Shantel and Hickie, Ian B and Naismith, Sharon L},
	month = feb,
