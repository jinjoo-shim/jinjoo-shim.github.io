	keywords = {Adult, Female, Humans, Male, Middle Aged, Severity of Illness Index, Young Adult, Cohort Studies, United States, Chronic Disease, Retrospective Studies, Models, Economic, Cost of Illness, Combined Modality Therapy, Health Care Costs, Insurance Coverage, Managed Care Programs, Multiple Sclerosis},
	pages = {e368--e374},
