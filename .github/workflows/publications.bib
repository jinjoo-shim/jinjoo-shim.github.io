@article{hastings_conceptual_2021,
	title = {Conceptual and {Analytical} {Overlap} {Between} {Allostatic} {Load} and {Systemic} {Biological} {Aging} {Measures}: {Analyses} {From} the {National} {Survey} of {Midlife} {Development} in the {United} {States}},
	volume = {77},
	issn = {1079-5006},
