	keywords = {Female, Humans, Male, Breast Neoplasms, Risk Assessment, Cardiovascular Diseases, Kaplan-Meier Estimate, Data Interpretation, Statistical, Evidence-Based Medicine},
	pages = {1105--1117},
	file = {Full Text:/Users/jishim/Zotero/storage/KFZY3M7W/Uno et al. - 2011 - On the C-statistics for evaluating overall adequac.pdf:application/pdf},
}

