	journal = {Nature Reviews Endocrinology},
	author = {Chaput, Jean-Philippe and McHill, Andrew W. and Cox, Rebecca C. and Broussard, Josiane L. and Dutil, Caroline and da Costa, Bruno G. G. and Sampasa-Kanyinga, Hugues and Wright, Kenneth P.},
