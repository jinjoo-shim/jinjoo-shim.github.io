	keywords = {Humans, Sleep, Models, Theoretical, Wakefulness, Wearable Electronic Devices, Circadian Rhythm, sleep, circadian rhythm, Work Schedule Tolerance, Quality of Life, shift work, wearable device, Alertness, irregular sleep, mathematical model, mobile application, sleep intervention},
