	pmcid = {PMC10398269},
	keywords = {Adult, Aged, Female, Humans, Male, Middle Aged, Aged, 80 and over, Young Adult, Adolescent, United States, Arthritis, Rheumatoid, Databases, Factual, Insurance Claim Review, Medicare, Quality of Health Care, Health Care Costs, Antirheumatic Agents},
	pages = {1472--1481},
	file = {Full Text:/Users/jishim/Zotero/storage/MIYMVNH5/Shafrin et al. - 2016 - Geographic Variation in the Quality and Cost of Ca.pdf:application/pdf},
}

