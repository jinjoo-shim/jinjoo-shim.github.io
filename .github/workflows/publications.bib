	author = {Thorsen-Meyer, Hans-Christian and Placido, Davide and Kaas-Hansen, Benjamin Skov and Nielsen, Anna P. and Lange, Theis and Nielsen, Annelaura B. and Toft, Palle and Schierbeck, Jens and Str√∏m, Thomas and Chmura, Piotr J. and Heimann, Marc and Belling, Kirstine and Perner, Anders and Brunak, S√∏ren},
