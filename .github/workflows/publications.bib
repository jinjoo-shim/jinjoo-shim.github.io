	note = {Pages: 2022.06.05.494875
Section: New Results},
	file = {Full Text PDF:/Users/jishim/Zotero/storage/2YIV8TJG/Ding et al. - 2022 - Statistical Methods for Detecting Circadian Rhythm.pdf:application/pdf},
}

