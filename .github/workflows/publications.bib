	month = jan,
	year = {1997},
	pmid = {9130329},
	keywords = {Aged, Female, Humans, Male, Middle Aged, Severity of Illness Index, Depression, Sleep, Wakefulness, Health Status, Circadian Rhythm, Dementia, Light, Nursing Homes},
	pages = {18--23},
}

