	keywords = {Physical activity, Sleep, Executive function, Sedentary behavior, Self-regulation},
	pages = {332--342},
	file = {Full Text PDF:/Users/jishim/Zotero/storage/MJJJV2M6/Fanning et al. - 2017 - Replacing sedentary time with sleep, light, or mod.pdf:application/pdf},
}

